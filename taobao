<script>
(function (win) {
    function _record (logkey, chksum) {
        if (logkey && chksum) {
            (win.goldlog_queue || (win.goldlog_queue = [])).push({
                action: "goldlog.record",
                arguments: [logkey, '', '', chksum]
            });
        }
    }

    function _getDataValue (domNode, dataKey) {
        var re = '';

        if (domNode && domNode.getAttribute) {
            re = domNode.getAttribute('data-' + dataKey) || '';
        }

        return re;
    }
     
    var linkDOMNodeArr = document.getElementById('web-game').getElementsByTagName('a');
    for (var i = 0, len = linkDOMNodeArr.length; i < len; i++) {
        linkDOMNodeArr[i].onclick = function (ev) {
            _record(_getDataValue(this, 'logkey'), _getDataValue(this, 'chksum'));
        }
    }
})(window);
</script>                &lt;/textarea&gt;
            </div>

        </div>
    </div>
</div>

<script>
    /**
     * 游戏的switchable处理 & 资源引用
     * @author Mu Yun <muyun.my@taobao.com>
     * @date 2013-02-27
     */


        KISSY.use('sizzle, node, dom, datalazyload, switchable', function (S ,Sizzle, Node, DOM, DataLazyLoad, Switchable) {
                var currentIndex = 0,
                    containerNode = Node.one('#J_Game'),
                    tabs = new Switchable.Tabs(containerNode, {
                        switchTo: currentIndex,
                        effect: 'scrollx',
                        easing: 'easeOutStrong',
                        viewSize: [278],
                        circular: true,
                        autoplay: false,
                        markupType: 1,
                        triggerCls: 'J_GameSWTrigger',
                        panelCls: 'J_GameSWPanel',
                        lazyTextareaClass: 'ks-datalazyload-custom',
                        aria: true,
                        delay: 0
                    });

                /* 事件绑定 */
                containerNode.delegate('click', '.J_GameSWTrigger', function (ev) {
                    // 导航不可点击
                    ev.preventDefault();
                    goldlogFn(S.one(ev.target).attr('data-goldlog'), '', '47700902');
                });

                tabs.on('beforeSwitch', function (ev) {
                    // 让导航元素聚焦以触发容器内弹层的blur事件来收起弹层
                    this.triggers[ev.toIndex].focus();
                    //hack
                        var tbrCardInputPanel = Node.one("#tbr-card-cat-input-panel");
                        if(ev.toIndex == 1 && tbrCardInputPanel && tbrCardInputPanel.css('display')!='none'){
                            Node.one("#tbr-card-cat-input")[0].focus();
                     }
                    // 隐藏非当前容器，防止tab操作被干扰
                    // setTabIndex(this.panels[currentIndex], -1);
                    DOM.css(this.panels[currentIndex], 'visibility', 'hidden');
                    currentIndex = ev.toIndex;
                    // setTabIndex(this.panels[currentIndex], 0);
                    DOM.css(this.panels[currentIndex], 'visibility', 'visible');
                });

                tabs.on('switch', function (ev) {
                    // 使当前panel容器元素不可聚焦
                    DOM.attr(this.panels[ev.currentIndex], 'tabindex', '-1');
                    goldlogFn('/wanle.12.' + (ev.currentIndex + 1), '', '47700902');
                });

                /* 初始化 */
                S.each(tabs.panels, function (panel, index) {
                    // 隐藏非当前容器，防止tab操作被干扰
                    // setTabIndex(panel, index == currentIndex ? 0 : -1);
                    DOM.css(panel, 'visibility', index == currentIndex ? 'visible' : 'hidden');
                });
                // 使当前panel容器元素不可聚焦
                DOM.attr(tabs.panels[tabs.activeIndex], 'tabindex', '-1');

                // 区分新老首页
                if (S.one('body').attr('data-spm') == '1000386') {
                    containerNode.addClass('new-service-game-fp1');
                } else {
                    containerNode.removeClass('new-service-game-fp1');
                    containerNode.addClass('new-service-game-fp2');
                }

               // S.one('#tbr-game-loading').hide();
                containerNode.show();

                goldlogFn('/wanle.12.1', '', '47700902');           
          
                /**
                 * 发送黄金令箭
                 * @param {String} logkey
                 * @param {String} gokey
                 * @param {String} chksum
                 */
                function goldlogFn (logkey, gokey, chksum) {
                    if(typeof goldlog !== 'undefined'){
                        goldlog.record(logkey, "", "", chksum);
                    }
                }
          
                /**
                 * 添加统计代码，测试改版之后的效率提升.
                 * @author liesun.wjb@taobao.com
                 * @date   2013.05.23
                 */
                try{
                    if(S.one('#J_Convenience').attr('data-activeindex') === '1'){
                        (window.goldlog_queue || (window.goldlog_queue = [])).push({
                          action : 'goldlog.emit',
                          arguments: ['tb_index_convenience', {module: 'game', time: new Date-g_config.startDate}]
                        }); 
                    }
                }catch(e){}            
        });
</script><!-- END -->		</textarea>
	</div>
